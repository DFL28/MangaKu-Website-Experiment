{
  "name": "mangaku-website",
  "version": "1.0.0",
  "description": "Website baca manga subtitle Indonesia dengan fitur mirror dari berbagai sumber",
  "main": "api-server.js",
  "scripts": {
    "start": "node api-server.js",
    "dev": "node -r dotenv/config api-server.js dotenv_config_path=.env.local",
    "dev:watch": "nodemon -r dotenv/config api-server.js dotenv_config_path=.env.local",
    "prod": "node -r dotenv/config api-server.js dotenv_config_path=.env.production",
    "vercel-build": "echo 'Static files will be served by Vercel'"
